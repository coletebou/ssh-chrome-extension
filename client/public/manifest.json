{
  "manifest_version": 3,
  "name": "Nebula Terminal",
  "version": "1.0.0",
  "description": "SSH terminal in your browser side panel. Connect to servers via WebSocket relay.",
  "icons": {
    "128": "icon-128.png"
  },
  "side_panel": {
    "default_path": "index.html"
  },
  "permissions": [
    "sidePanel",
    "storage"
  ],
  "action": {
    "default_title": "Open Terminal"
  }
}
